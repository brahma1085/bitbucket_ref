<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<html class="no-js" data-ng-app="iTap">

<head>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
<title>iSATT</title>
<meta name="description" content="" />
<meta name="viewport" content="width=device-width" />
<link rel="stylesheet" href="cssITAP/normalize.css" />
<link rel="stylesheet" href="cssITAP/main.css" />
<link rel="stylesheet" href="cssITAP/login.css" />
<link rel="stylesheet" href="cssITAP/jquery-ui.css" />
<link rel="stylesheet" href="cssITAP/chosen.css" />
<script src="js-isatt/vendor/jquery-1.9.0.min.js"></script>
<script src="js-isatt/vendor/jquery-ui.js"></script>
<script src="js-isatt/chosen.jquery.min.js"></script>
<script src="angular/angular.min.js"></script>

<script src="angular-dragdrop.js"></script>
<script src="angular-dragdrop.min.js"></script>

<script src="js/spagojs/sbisdk-all-production.js"></script>

<script src="controllersITAP.js"></script>
<script type="text/javascript" src="js-isatt/adminMain.js"></script>
<script type="text/javascript" src="js-isatt/main.js"></script>

<script src="js/spagojs/sbisdk-all-production.js"></script>

<script src="controllersITAP.js"></script>
<script type="text/javascript" src="js-isatt/adminMain.js"></script>
<script type="text/javascript" src="js-isatt/main.js"></script>

<link rel="stylesheet" href="cssITAP/jquery-ui.css" />
<link rel="stylesheet" href="cssITAP/styles.css" />

<link rel="stylesheet" href="cssITAP/chosen.css" />


<script src="js-isatt/DateUtil.js"></script>
</head>
<body id="login">
	<form novalidate name='f' action='j_spring_security_check'  novalidate  method='POST'>
		<div id="login_page_container">
			<header class="header">
				<div id="logo">
					<img src="img-isatt/logo.png" alt="iTap" class="logoImg">
				</div>
			<%-- 	<c:choose>
      <c:when test="${not empty error}">
           <div class="error">
            ${sessionScope["SPRING_SECURITY_LAST_EXCEPTION"].message}
            </div>
      </c:when>
      <c:otherwise>
     <div></div>
      </c:otherwise>
</c:choose> --%>
				<c:if  test="${not empty param['error']}">
					<div class="error">The Username or Password entered is incorrect</div>
				</c:if> 
			</header>
			<div class="inputFields">
				
					<label for="user">Username</label> <input type="text"
						name='j_username' data-ng-model="j_username" required /><br />
					<div class="error" data-ng-show="f.j_username.$dirty && f.j_username.$invalid">
						<span
							data-ng-show="f.j_username.$error.required">
							Username is required. </span>
					</div>
					<label for="password">Password</label> <input type="password"
						name='j_password' data-ng-model="j_password" required><br />
						<div class="error" data-ng-show="f.j_password.$dirty && f.j_password.$invalid">
						<span
							data-ng-show="f.j_password.$error.required">
							Password is required. </span>
							
					</div>
					<br />
					 <input data-ng-disabled="f.j_username.$invalid || f.j_password.$invalid"
						type="submit" name="login" value="Login" id="loginBtn" />
				
			</div>
			<footer class="footer">
				<div class="bottomNav">
					<span class="floatright"><a href="forgotPassword.html" class="forgotPass link">Forgot
							Password?</a></span>
					<!--  <span class="floatright"><a href="#" class="TandC link">Terms & Conditions</a></span> -->
				</div>
			</footer>
		</div>
	</form>
</body>
</html>