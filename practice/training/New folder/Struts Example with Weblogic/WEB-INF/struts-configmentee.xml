         	<forward name="NoAccess" path="/jsp/noaccess.jsp" />
		    <forward name="NoSession" path="/jsp/nosession.jsp" />
    </action> 
 
    
    <action path="/InputProgressPage"
            name="MenteeMentorLoadForm"
            type="com.ge.crd.ementoring.action.mentee.InputProgressAction"        
			scope="session">                   
            <forward name="Success" path="/jsp/mentee/InputProgress.jsp" />
         	<forward name="NoAccess" path="/jsp/noaccess.jsp" />
		    <forward name="NoSession" path="/jsp/nosession.jsp" />
    </action> 
  
   <action path="/InputProgressOfPeople"
            name="MenteeMentorLoadForm"
            type="com.ge.crd.ementoring.action.mentee.InputProgressAction"        
			scope="session">                   
           <forward name="Success" path="/jsp/mentee/SuccessInputProgress.jsp" />
		   <forward name="Failure" path="/jsp/NotAbleToProcessRequest.jsp" />
		   <forward name="NoAccess" path="/jsp/noaccess.jsp" />
		   <forward name="NoSession" path="/jsp/nosession.jsp" />           
