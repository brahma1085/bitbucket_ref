    <form-bean name="registeredMenteeForm" type="com.ge.crd.ementoring.formbean.mentee.RegisteredMenteeForm"/>
    <form-bean name="MenteeProgressForm" type="com.ge.crd.ementoring.formbean.mentee.MenteeProgressForm"/>
  </form-beans>
  
  <action-mappings>
   
    <action path="/menteeRegistration"
            name="menteeForm"
            type="com.ge.crd.ementoring.action.mentee.MenteeRegAction" 
    	    scope="request">
    	    <forward name="Success" path="/jsp/mentee/MenteeRegistration.jsp" />
		    <forward name="Failure" path="/jsp/NotAbleToProcessRequest.jsp" />
		    <forward name="NoAccess" path="/jsp/noaccess.jsp" />
		    <forward name="NoSession" path="/jsp/nosession.jsp" />    	  
    </action>
    
    <action path="/MenteeRegistrationPage" 
  	    name="menteeForm" 
  	    type="com.ge.crd.ementoring.action.mentee.MenteeRegistation" 
	  	scope="request">
	  	<forward name="Success" path="/jsp/mentee/SuccessRegistration.jsp" />
  		<forward name="NoAccess" path="/jsp/noaccess.jsp" />
		<forward name="NoSession" path="/jsp/nosession.jsp" />
    </action>
   
    <action path="/SaveMenteeProfile" 
  	    name="menteeForm" 
  	    type="com.ge.crd.ementoring.action.mentee.SaveMenteeAction" 
	  	scope="request">
	  	<forward name="Success" path="/jsp/mentee/SuccessSaveMenteeDetails.jsp" />
  		<forward name="NoAccess" path="/jsp/noaccess.jsp" />
		<forward name="NoSession" path="/jsp/nosession.jsp" />
    </action>
   
    <action path="/MenteeEditRegistration"
            name="MenteeEditForm"
            type="com.ge.crd.ementoring.action.mentee.MenteeUpdateAction" 
            scope="request">
    	  <forward name="Success" path="/jsp/mentee/MenteeEditRegistration.jsp" />
		  <forward name="Failure" path="/jsp/NotAbleToProcessRequest.jsp" />
		  <forward name="NoAccess" path="/jsp/noaccess.jsp" />
		  <forward name="NoSession" path="/jsp/nosession.jsp" />    	  
    </action>
   
    <action path="/MenteeUpdateRegistration"
            name="MenteeEditForm"
            type="com.ge.crd.ementoring.action.mentee.MenteeUpdateAction" 
    	    scope="request">
    	  <forward name="Success" path="/jsp/mentee/SuccessUpdation.jsp" />
		  <forward name="NoAccess" path="/jsp/noaccess.jsp" />
		  <forward name="NoSession" path="/jsp/nosession.jsp" />    	  
    </action>
      
    <action path="/MenteeSearchingHisMentor"
             type="com.ge.crd.ementoring.action.mentee.FindingMentorAction"        
             name="MenteeFindingMentorForm"
			 scope="session">                   
           <forward name="Success" path="/jsp/mentee/MenteeFindingMentor.jsp" />
           <forward name="Failure" path="/jsp/NotAbleToProcessRequest.jsp" />
           <forward name="AlreadySelected" path="/jsp/mentee/MenteeSelectedMentor.jsp" />
       	   <forward name="NoAccess" path="/jsp/noaccess.jsp" />
		  <forward name="NoSession" path="/jsp/nosession.jsp" />
    </action> 
 
    <action path="/MenteeSearchResult"
            name="MenteeFindingMentorForm"
            scope="session"
            type="com.ge.crd.ementoring.action.mentee.MenteeFindingMentorAction" >
    	    <forward name="Success" path="/jsp/mentee/MenteeFindingMentor.jsp" />
            <forward name="Failure" path="/jsp/NotAbleToProcessRequest.jsp" />
            <forward name="SendRequest" path="/jsp/mentee/MenteeSendRequestToMentor.jsp" />
		    <forward name="NoAccess" path="/jsp/noaccess.jsp" />
		    <forward name="NoSession" path="/jsp/nosession.jsp" />            
    </action>
    
    <action path="/MenteeSendingRequest"
            name="MenteeFindingMentorForm"
            scope="session"
            type="com.ge.crd.ementoring.action.mentee.MenteeFindingMentorAction" >
