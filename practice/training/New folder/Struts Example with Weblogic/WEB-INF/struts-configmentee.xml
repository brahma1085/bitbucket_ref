            <forward name="Failure" path="/jsp/NotAbleToProcessRequest.jsp" />
            <forward name="SendRequest" path="/jsp/mentee/MenteeSendRequestToMentor.jsp" />
		    <forward name="NoAccess" path="/jsp/noaccess.jsp" />
		    <forward name="NoSession" path="/jsp/nosession.jsp" />            
    </action>
    
    <action path="/ViewMentorDetailsPage"
            name="MenteeFindingMentorForm"
            type="com.ge.crd.ementoring.action.mentee.MentorDetailsAction"        
			scope="session">                   
            <forward name="Success" path="/jsp/mentee/MentorDetails.jsp" />
		    <forward name="Failure" path="/jsp/NotAbleToProcessRequest.jsp" />
		    <forward name="NoAccess" path="/jsp/noaccess.jsp" />
		    <forward name="NoSession" path="/jsp/nosession.jsp" />           
