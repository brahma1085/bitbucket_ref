<?xml version="1.0" encoding="iso-8859-1"?>
<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN"
          "http://struts.apache.org/dtds/struts-config_1_1.dtd">

<struts-config>

  <form-beans>
  	<form-bean name="menteeForm" type="com.ge.crd.ementoring.formbean.mentee.MenteeRegistrationForm" />
	<form-bean name="MenteeEditForm" type="com.ge.crd.ementoring.formbean.mentee.MenteeEditForm" />	
	<form-bean name="MenteeFindingMentorForm" type="com.ge.crd.ementoring.formbean.mentee.MenteeFindingMentorForm" />
