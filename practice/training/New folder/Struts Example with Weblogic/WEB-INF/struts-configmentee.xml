    	    <forward name="Success" path="/jsp/mentee/SuccessUpdateInputProgress.jsp" />
		    <forward name="Failure" path="/jsp/NotAbleToProcessRequest.jsp" />
		    <forward name="NoAccess" path="/jsp/noaccess.jsp" />
		    <forward name="NoSession" path="/jsp/nosession.jsp" />    	  
    </action>
   
     <action path="/SuperAdminViewRegMenteeList"
            name="MenteeProfileForm"
            type="com.ge.crd.ementoring.action.mentee.SuperAdminMenteeListAction"        
			scope="request">                   
           <forward name="Success" path="/jsp/mentee/ViewMenteeList.jsp" />
		   <forward name="NoAccess" path="/jsp/noaccess.jsp" />
		   <forward name="NoSession" path="/jsp/nosession.jsp" />           
    </action> 
    
    <action path="/SuperAdminViewRegMenteeResultList"
