	<enterprise-beans>
		<session>
			<ejb-name>BACKOFFICEEJBWEB</ejb-name>
			<home>backOfficeServer.BackOfficeHome</home>
			<remote>backOfficeServer.BackOfficeRemote</remote>
			<ejb-class>backOfficeServer.BackOfficeBean</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>

			<ejb-local-ref>
				<ejb-ref-name>COMMONLOCALWEB</ejb-ref-name>
				<ejb-ref-type>Session</ejb-ref-type>
				<local-home>commonServer.CommonHome</local-home>
				<local>commonServer.CommonLocal</local>
				<ejb-link>COMMONEJBWEB</ejb-link>
			</ejb-local-ref>
			<ejb-local-ref>
				<ejb-ref-name>SHARELOCALWEB</ejb-ref-name>
				<ejb-ref-type>Session</ejb-ref-type>
				<local-home>shareServer.ShareLocalHome</local-home>
