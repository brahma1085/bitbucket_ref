			</ejb-local-ref>
			<ejb-local-ref>
				<ejb-ref-name>CASHLOCAL</ejb-ref-name>
				<ejb-ref-type>Session</ejb-ref-type>
				<local-home>cashServer.CashLocalHome</local-home>
				<local>cashServer.CashLocal</local>
				<ejb-link>CASHEJB</ejb-link>
			</ejb-local-ref>
		</session>

		<session>
			<ejb-name>COMMONEJBWEB</ejb-name>
			<home>commonServer.CommonHome</home>
			<remote>commonServer.CommonRemote</remote>
			<local-home>commonServer.CommonLocalHome</local-home>
			<local>commonServer.CommonLocal</local>
			<ejb-class>commonServer.CommonBean</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>
			<security-identity>
				<use-caller-identity />
			</security-identity>
		</session>


