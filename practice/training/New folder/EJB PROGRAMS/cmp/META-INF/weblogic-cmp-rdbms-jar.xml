<!DOCTYPE weblogic-rdbms-jar PUBLIC 
 '-//BEA Systems, Inc.//DTD WebLogic 7.0.0 EJB RDBMS Persistence//EN'
 'http://www.bea.com/servers/wls700/dtd/weblogic-rdbms20-persistence-700.dtd'>
<weblogic-rdbms-jar>
  <weblogic-rdbms-bean>
    <ejb-name>cmpp</ejb-name>
    <data-source-name>bilaltxJNDI</data-source-name>
    <table-map>
      <table-name>accounts</table-name>
      <field-map>
        <cmp-field>accountid</cmp-field>
        <dbms-column>accountid</dbms-column>
      </field-map>
      <field-map>
        <cmp-field>balance</cmp-field>
        <dbms-column>balance</dbms-column>
      </field-map>
      <field-map>
        <cmp-field>ownername</cmp-field>
        <dbms-column>ownername</dbms-column>
      </field-map>
    </table-map>

  </weblogic-rdbms-bean>

  <create-default-dbms-tables>True</create-default-dbms-tables>

</weblogic-rdbms-jar>

