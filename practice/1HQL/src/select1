String hqlQuery = "DELETE Student  WHERE studentNo=:sno and studentName=:sname";
			Query query = session.createQuery(hqlQuery);
			query.setString("sname", "N@IT-U");
			query.setString("sno", "1");
			int noOfQueries = query.executeUpdate();
			System.out.println("number of queries updated==>" + noOfQueries);
==================================================================================================
	String hqlQuery = "UPDATE Student SET studentName=:sname WHERE studentNo=:sno";
			Query query = session.createQuery(hqlQuery);
			query.setString("sname", "N@IT-U");
			query.setString("sno", "1");
			int noOfQueries = query.executeUpdate();
			System.out.println("number of queries updated==>" + noOfQueries);
