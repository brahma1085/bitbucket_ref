String hqlQuery = "SELECT MIN(studentNo), MAX(studentNo), AVG(studentNo) FROM Student";
			Query query = session.createQuery(hqlQuery);
			Object[] studentObjects = null;
			Iterator iterator = query.iterate();
			if (iterator.hasNext()) {
				studentObjects = (Object[]) iterator.next();
				System.out.println("minimum value of Student==>"
						+ studentObjects[0]);
				System.out.println("maximum value of Student==>"
						+ studentObjects[1]);
				System.out.println("average value of Student==>"
						+ studentObjects[2]);
				System.out
						.println("the Object array contains the following types of objects");
				System.out.println("MIN-type==>"
						+ studentObjects[0].getClass().getName());
				System.out.println("MAX-type==>"
						+ studentObjects[1].getClass().getName());
				System.out.println("AVG-type==>"
						+ studentObjects[2].getClass().getName());
			}
===================================================================================================
String hqlQuery = "SELECT MIN(studentNo), MAX(studentNo), AVG(studentNo) FROM Student";
			Query query = session.createQuery(hqlQuery);
			List studentList = query.list();
			System.out.println("number of records==>" + studentList.size());
			System.out.println("break point");
			Iterator iterator = studentList.iterator();
			Object[] studentObjects = null;
			while (iterator.hasNext()) {
				studentObjects = (Object[]) iterator.next();
				System.out.println("minimum value of Student==>"
						+ studentObjects[0]);
				System.out.println("maximum value of Student==>"
						+ studentObjects[1]);
				System.out.println("average value of Student==>"
						+ studentObjects[2]);
				System.out
						.println("the Object array contains the following types of objects");
				System.out.println("MIN-type==>"
						+ studentObjects[0].getClass().getName());
				System.out.println("MAX-type==>"
						+ studentObjects[1].getClass().getName());
				System.out.println("AVG-type==>"
						+ studentObjects[2].getClass().getName());
			}
