String query = "SELECT studentNo,studentName FROM Student";
			Iterator studentList = session.createQuery(query).iterate();
			System.out
					.println("break point and check the list in variables window");
			Object[] studentObj = null;
			while (studentList.hasNext()) {
				studentObj = (Object[]) studentList.next();
				System.out.println("student number==>" + studentObj[0]);
				System.out.println("student name==>" + studentObj[1]);
				System.out
						.println("break point and check the variables window");
			}
===================================================================================================
	String hqlQuery = "FROM Student ORDER BY studentNo ASC";
			List studentList = session.createQuery(hqlQuery).list();
			System.out.println("no of rows effected ==>" + studentList.size());
			Iterator iterator = studentList.iterator();
			while (iterator.hasNext()) {
				Student student = (Student) iterator.next();
				System.out
						.println("Student Number==>" + student.getStudentNo());
				System.out
						.println("Student Name==>" + student.getStudentName());
			}
===================================================================================================
	String hqlQuery = "FROM Student ORDER BY studentNo DESC";
			List studentList = session.createQuery(hqlQuery).list();
			System.out.println("no of rows effected ==>" + studentList.size());
			Iterator iterator = studentList.iterator();
			while (iterator.hasNext()) {
				Student student = (Student) iterator.next();
				System.out
						.println("Student Number==>" + student.getStudentNo());
				System.out
						.println("Student Name==>" + student.getStudentName());
