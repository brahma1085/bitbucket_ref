<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>EasyEPFInfo</display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<description>MySQL Test App</description>
	<resource-ref>
		<description>DB Connection</description>
		<res-ref-name>jdbc/brahmaJNDI</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
	</resource-ref>
	<servlet>
		<display-name>EmployeeDetails</display-name>
		<servlet-name>EmployeeDetailsServlet</servlet-name>
		<servlet-class>com.personal.epf.servlet.EmployeeDetailsServlet</servlet-class>
	</servlet>
	<servlet>
		<display-name>EmployeeRegistration</display-name>
		<servlet-name>EmployeeRegistrationServlet</servlet-name>
		<servlet-class>com.personal.epf.servlet.EmployeeRegistrationServlet</servlet-class>
	</servlet>
	<servlet>
		<display-name>PFCalculation</display-name>
		<servlet-name>PFCalculationServlet</servlet-name>
		<servlet-class>com.personal.epf.servlet.PFCalculationServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>employeeregistration</servlet-name>
		<jsp-file>/WEB-INF/pages/employeeregistration.jsp</jsp-file>
	</servlet>
	<servlet>
		<servlet-name>employeepfdetails</servlet-name>
		<jsp-file>/WEB-INF/pages/employeepfdetails.jsp</jsp-file>
	</servlet>
	<servlet>
		<servlet-name>employeesummary</servlet-name>
		<jsp-file>/WEB-INF/pages/employeesummary.jsp</jsp-file>
	</servlet>
	<servlet>
		<servlet-name>content</servlet-name>
		<jsp-file>/WEB-INF/pages/content.jsp</jsp-file>
	</servlet>
	<servlet>
		<servlet-name>options</servlet-name>
		<jsp-file>/WEB-INF/pages/options.jsp</jsp-file>
	</servlet>
	<servlet>
		<servlet-name>header</servlet-name>
		<jsp-file>/WEB-INF/pages/header.jsp</jsp-file>
	</servlet>
	<servlet>
		<servlet-name>integration</servlet-name>
		<jsp-file>/WEB-INF/pages/integration.jsp</jsp-file>
	</servlet>
	<servlet>
		<servlet-name>ForwardServlet</servlet-name>
		<servlet-class>com.personal.epf.servlet.ForwardServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>employeeregistration</servlet-name>
		<url-pattern>/employeeregistration</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>employeepfdetails</servlet-name>
		<url-pattern>/employeepfdetails</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>employeesummary</servlet-name>
		<url-pattern>/employeesummary</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>content</servlet-name>
		<url-pattern>/content</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>options</servlet-name>
		<url-pattern>/options</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>header</servlet-name>
		<url-pattern>/header</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>integration</servlet-name>
		<url-pattern>/integration</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>EmployeeDetailsServlet</servlet-name>
		<url-pattern>/EmployeeDetailsServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>EmployeeRegistrationServlet</servlet-name>
		<url-pattern>/EmployeeRegistrationServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PFCalculationServlet</servlet-name>
		<url-pattern>/PFCalculationServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ForwardServlet</servlet-name>
		<url-pattern>/ForwardServlet</url-pattern>
	</servlet-mapping>
</web-app>